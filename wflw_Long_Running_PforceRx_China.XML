            <TABLEATTRIBUTE NAME ="Sql Query" VALUE ="SELECT DISTINCT&#xD;&#xA;         a.subj_name AS subject_area,&#xD;&#xA;         b.workflow_name AS workflow_name,&#xD;&#xA;         b.start_time AS workflow_start_time,&#xD;&#xA;         b.end_time AS workflow_end_time,&#xD;&#xA;         c.instance_name AS session_name,&#xD;&#xA;         c.start_time AS session_start_time,&#xD;&#xA;         c.end_time AS session_end_time,&#xD;&#xA;         ROUND ( (NVL (c.end_time, SYSDATE) - c.start_time) * 1440, 2) AS current_running_time_mins,&#xD;&#xA;         avg_run_time,&#xD;&#xA;         c.server_name&#xD;&#xA;    FROM INFA961_PFORCERX_REP_P01.opb_subject a,&#xD;&#xA;         INFA961_PFORCERX_REP_P01.opb_wflow_run b,&#xD;&#xA;         INFA961_PFORCERX_REP_P01.opb_task_inst_run c,&#xD;&#xA;         (/* Average run time */&#xD;&#xA;          SELECT   workflow_id,&#xD;&#xA;                   task_id,&#xD;&#xA;                   ROUND (AVG ( (NVL (end_time, SYSDATE) - start_time) * (24 * 60)), 2) avg_run_time&#xD;&#xA;              FROM INFA961_PFORCERX_REP_P01.opb_task_inst_run&#xD;&#xA;             WHERE     task_type = 68&#xD;&#xA;                   AND TRUNC (start_time) BETWEEN ADD_MONTHS (TRUNC (SYSDATE - 2), -2)&#xD;&#xA;                                              AND TRUNC (SYSDATE - 2) -- Last 2 months excluding yesterday&#xD;&#xA;          GROUP BY workflow_id, task_id&#xD;&#xA;            HAVING ROUND (AVG ( (NVL (end_time, SYSDATE) - start_time) * (24 * 60)), 2) &gt; 20 -- Benchmark time set for the alert&#xD;&#xA;          ) avg_time&#xD;&#xA;   WHERE     a.subj_id = b.subject_id&#xD;&#xA;         AND a.subj_id = c.subject_id&#xD;&#xA;         AND b.workflow_id = c.workflow_id&#xD;&#xA;         AND b.workflow_run_id = c.workflow_run_id&#xD;&#xA;         AND c.task_type = 68&#xD;&#xA;         AND c.start_time &gt;= TRUNC (SYSDATE - 1)                       -- Sessions started yesterday&#xD;&#xA;         AND ROUND ( (NVL (c.end_time, SYSDATE) - c.start_time) * 1440, 2) &gt; avg_run_time -- Current run time exceeds average run time of last 1 month&#xD;&#xA;         AND c.workflow_id = avg_time.workflow_id&#xD;&#xA;         AND c.task_id = avg_time.task_id&#xD;&#xA;ORDER BY 8 DESC,&#xD;&#xA;         1,&#xD;&#xA;         2,&#xD;&#xA;         5"/>
